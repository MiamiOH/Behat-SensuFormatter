    "require": {
        "php": ">=7.0",
        "behat/behat": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/process": ">2.3,<4.0"
    },
    "authors": [
        {
            "name": "Don Kidd",
            "email": "kidddw@miamioh.edu"
        }
    ],

    "autoload": {
        "psr-4": {
